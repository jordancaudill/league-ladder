{
  "name": "league-ladder",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev-frontend": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-swagger-docs": "ts-node src/backend/utils/generate-docs.ts",
    "clean": "rimraf ./dist/*",
    "build-backend": "npm-run-all clean build-swagger-docs tsc-backend",
    "dev-backend": "nodemon --watch src -e ts --exec npm run dev:start-backend",
    "start-backend": "node dist/src/server.js",
    "dev:start-backend": "npm-run-all build-backend start-backend",
    "test:unit": "vitest --environment jsdom --root src/",
    "test:e2e": "start-server-and-test preview :4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' :4173 'cypress open --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "tsc-backend": "tsc --project ./tsconfig.backend.json",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.3.0",
    "@fortawesome/free-brands-svg-icons": "^6.3.0",
    "@fortawesome/free-regular-svg-icons": "^6.3.0",
    "@fortawesome/free-solid-svg-icons": "^6.3.0",
    "@fortawesome/vue-fontawesome": "^3.0.3",
    "axios": "^1.3.3",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-session": "^1.17.3",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^9.0.0",
    "openapi-types": "^12.1.0",
    "passport": "^0.6.0",
    "passport-discord": "^0.1.4",
    "passport-local": "^1.0.0",
    "pg": "^8.9.0",
    "pg-hstore": "^2.3.4",
    "pinia": "^2.0.28",
    "sequelize": "^6.28.0",
    "swagger-ui-express": "^4.6.0",
    "tedious": "^15.1.2",
    "ts-json-schema-generator": "^1.2.0",
    "uuid": "^9.0.0",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/helmet": "^4.0.0",
    "@types/jsdom": "^20.0.1",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.11.12",
    "@types/passport": "^1.0.12",
    "@types/passport-discord": "^0.1.6",
    "@types/passport-local": "^1.0.35",
    "@types/sequelize": "^4.28.14",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^9.0.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.13",
    "cypress": "^12.0.2",
    "eslint": "^8.22.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^20.0.3",
    "msw": "^1.0.1",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "sequelize-cli": "^6.6.0",
    "sqlite3": "^5.1.4",
    "start-server-and-test": "^1.15.2",
    "supertest": "^6.3.3",
    "supertest-prefix": "^1.0.2",
    "tailwindcss": "^3.2.7",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "~4.7.4",
    "vite": "^4.0.0",
    "vitest": "^0.25.6",
    "vue-tsc": "^1.0.12",
    "whatwg-fetch": "^3.6.2"
  }
}
